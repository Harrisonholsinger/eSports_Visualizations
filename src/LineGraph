library("ggplot2")
library("tidyverse")


ggplot(eSportsData, aes(x = Game, y = Number.of.Players..Millions., 
                            group = 1)) + geom_line(color = "red") + geom_point() +
  labs(title="Number of Players of Selected eSports Games Worldwide as of August 2017",
       x ="Game", y = "Number of Players (Millions)")
